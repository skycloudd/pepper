func main() -> int =
    num(boolean(numm(1)))

func numm(a: int) -> int =
    a

func boolean(a: int) -> int =
    a

func num(a: int) -> int =
    rec_with_args(1, true, numm)

func recursive() -> ! =
    recursive()

func rec_with_args(a: int, b: bool, c: func (int) -> int) -> int =
    rec_with_args(a, b, c)

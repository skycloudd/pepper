func main() {
    var planets = ["Venus", "Earth", "Mars"];

    for (idx, planet) in enumerate(planets) {
        println("planet $(idx + 1): $(planet)");
    };

    var n = 10;
}

func fibonacci(n: Int32) -> Int32 {
    match n {
        n where n < 2 => n,
        _ => fibonacci(n - 1) + fibonacci(n - 2),
    }
}
